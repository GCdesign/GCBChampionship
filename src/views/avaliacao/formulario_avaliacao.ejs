    <form action="/avaliacao/salvar" method="POST" style="margin-left:5px;">
        <div class="form-group">
        <label style="margin-left:1px;">Classe</label>
        <input type="text" class="form-control" id="classe"  name="classe" placeholder="A ou B">
        
        <label style="margin-left:1px;">Participante</label>
        <input type="text" class="form-control" id="participante" name="participante" placeholder="Apelido do capoeirista">
        
        <label style="margin-left:1px;">Ritmo</label>
        <input type="text" class="form-control" id="ritmo" name="ritmo" placeholder="1-Banguela 2-Regional">
        
        <label style="margin-left:1px;">Número de Inscrição</label>
        <input type="text" class="form-control" id="inscricao" name="inscricao" placeholder="Número de inscrição">
        

        <label style="margin-left:1px;">Nota</label>
        <input type="text" class="form-control" id="nota" name="nota" placeholder="Nota">
        <br/>
 
        <input type="submit" value="Enviar Nota" class="btn btn-secondary d-inline float-right" style="margin-right:100px;">
    </form>
    <div class=" float-left " style="margin-left:100px;">
            <form action="/home" method="GET">
            <input type="submit" value="Voltar para o início" class="btn d-inline p-2 btn-secondary">
            <br/>
            </form>
